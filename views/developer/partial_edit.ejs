<!-- main - a row holds columns -->
<div class=row>
    <div class="col col-xs-12">
  
      <div class="form-group">
        <label for="email">Email</label>
        <input type="email" class="form-control" id="email" name="email" value="<%= developer.email %>" required>
      </div>
  
      <div class="form-group">
        <label for="given">Given name</label>
        <input type="text" class="form-control" id="given" name="given" value="<%= developer.given %>">
      </div>
  
      <div class="form-group">
        <label for="family">Family name</label>
        <input type="text" class="form-control" id="family" name="family" value="<%= developer.family %>">
      </div>
  
      <div class="form-group">
        <label for="city">City </label>
        <input type="text" class="form-control" id="city" name="city" value="<%= developer.city %>">
      </div>
  
      <div class="form-group">
        <label for="state">State </label>
        <input type="text" class="form-control" id="state" name="state" value="<%= developer.state %>">
      </div>
  
      <div class="form-group">
        <label for="zip">zip </label>
        <input type="zipcode" class="form-control" id="zip" name="zip" value="<%= developer.zip %>">
      </div>
  
      <div class="form-group">
        <label for="country">Country </label>
        <input type="text" class="form-control" id="country" name="country" value="<%= developer.country %>">
      </div>
  
    </div>
  </div>
  
  <!-- PUT CLIENT SIDE CODE HERE -->
  <script>
   
    // prevent submission when hitting enter key - optional, but user-friendly 
    document.forms[0].onkeypress = function (e) {
      var key = e.charCode || e.keyCode || 0;
      if (key == 13) {
        e.preventDefault();
      }
    }
  
  // END CLIENT SIDE CODE  (close the script tag)
  </script>